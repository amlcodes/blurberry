{
  "name": "blueberry-browser",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "bun typecheck:node && bun typecheck:web",
    "start": "electron-vite preview",
    "dev": "bun --env-file .env.local electron-vite dev",
    "build": "bun --env-file .env.local typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "build && electron-builder --dir",
    "build:win": "build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/openai": "^2.0.30",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-slot": "^1.2.3",
    "ai": "^5.0.44",
    "class-variance-authority": "^0.7.1",
    "dotenv": "^17.2.2",
    "hnswlib-node": "^3.0.0",
    "lucide-react": "^0.548.0",
    "motion": "^12.23.24",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "sql.js": "^1.12.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^19.1.8",
    "@types/sql.js": "^1.4.9",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.1.0",
    "clsx": "^2.1.1",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  }
}
